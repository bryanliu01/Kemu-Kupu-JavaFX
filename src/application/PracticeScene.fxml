<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1080.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="application.PracticeSceneController">
   <children>
      <Button fx:id="submitButton" layoutX="564.0" layoutY="310.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="126.0" style="-fx-cursor: hand;" text="Submit">
         <font>
            <Font size="28.0" />
         </font></Button>
      <Label fx:id="titleLabel" alignment="CENTER" contentDisplay="CENTER" layoutY="27.0" prefHeight="38.0" prefWidth="1080.0" text="Label">
         <font>
            <Font name="System Italic" size="50.0" />
         </font>
      </Label>
      <TextField fx:id="answerField" layoutX="365.0" layoutY="178.0">
         <font>
            <Font size="28.0" />
         </font></TextField>
      <Button fx:id="skipButton" layoutX="383.0" layoutY="310.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="93.0" style="-fx-cursor: hand;" text="Skip">
         <font>
            <Font size="28.0" />
         </font></Button>
      <Slider fx:id="festivalSpeed" blockIncrement="0.1" layoutX="366.0" layoutY="456.0" majorTickUnit="0.25" max="1.5" min="0.5" minorTickCount="5" prefHeight="61.0" prefWidth="350.0" showTickMarks="true" snapToTicks="true" style="-fx-cursor: hand;" value="1.0" />
      <Label fx:id="festivalSpeedLabel" layoutX="415.0" layoutY="390.0" prefHeight="32.0" prefWidth="253.0" text="Text Speed: 1x">
         <font>
            <Font size="28.0" />
         </font></Label>
      <Button fx:id="repeatButton" layoutX="287.0" layoutY="547.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="206.0" style="-fx-cursor: hand;" text="Repeat Word">
         <font>
            <Font size="28.0" />
         </font></Button>
      <Label fx:id="blankWord" alignment="CENTER" contentDisplay="CENTER" layoutX="384.0" layoutY="94.0" prefHeight="44.0" prefWidth="315.0" textAlignment="CENTER">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Button fx:id="continueButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#moveToPracticeModuleScene" prefHeight="50.0" prefWidth="106.0" text="Back">
         <font>
            <Font size="28.0" />
         </font></Button>
      <Button fx:id="macronButton" layoutX="618.0" layoutY="547.0" mnemonicParsing="false" onAction="#macronButton" prefHeight="50.0" prefWidth="143.0" style="-fx-cursor: hand;" text="Macron">
         <font>
            <Font size="28.0" />
         </font>
      </Button>
      <Label fx:id="gameModeLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="365.0" layoutY="647.0" prefHeight="38.0" prefWidth="350.0" text="Current Mode: Practice">
         <font>
            <Font name="System Italic" size="28.0" />
         </font>
      </Label>
      <Label fx:id="selfReflection" layoutX="365.0" layoutY="255.0" prefHeight="32.0" prefWidth="350.0" text="Press Enter To Continue." visible="false">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
