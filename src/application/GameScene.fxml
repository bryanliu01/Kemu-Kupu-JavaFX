            <Font size="28.0" />
         </font>
      </Button>
      <Label fx:id="gameTimerLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="462.0" layoutY="242.0" prefHeight="46.0" prefWidth="157.0" text="Timer" textAlignment="CENTER">
