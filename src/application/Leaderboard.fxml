<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1080.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="application.LeaderboardController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="4.0" layoutY="14.0" prefHeight="69.0" prefWidth="1074.0" stylesheets="@application.css" text="Leaderboard">
         <font>
            <Font size="58.0" />
         </font>
      </Label>
      <TableView fx:id="table" layoutX="29.0" layoutY="72.0" prefHeight="521.0" prefWidth="1018.0" stylesheets="@TableViewStyle.css">
        <columns>
          <TableColumn fx:id="nameCol" editable="false" prefWidth="254" text="Name" />
          <TableColumn fx:id="scoreCol" editable="false" minWidth="0.0" prefWidth="254" sortType="DESCENDING" text="Score" />
          <TableColumn fx:id="topicCol" editable="false" minWidth="0.0" prefWidth="254" text="Topic" />
          <TableColumn fx:id="dateCol" editable="false" minWidth="0.0" prefWidth="254" text="Date" />
        </columns>
      </TableView>
      <Button fx:id="ReturnButton" layoutX="465.0" layoutY="631.0" mnemonicParsing="false" onAction="#returnToMainScene" prefHeight="50.0" prefWidth="146.0" style="-fx-cursor: hand;" text="Return">
         <font>
            <Font size="28.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
